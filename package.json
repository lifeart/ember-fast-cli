{
  "name": "ember-fast-cli",
  "version": "1.3.1",
  "description": "Super fast web-cli for ember-cli",
  "keywords": [
    "ember-addon",
    "ember-cli",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lifeart/ember-fast-cli.git"
  },
  "license": "MIT",
  "author": "Aleksandr Kanunnikov <lifeart92@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember-language-server": {
    "entry": "./index"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "resolutions": {
    "**/ini": "1.3.8",
    "**/node-fetch": "^2.6.1"
  },
  "dependencies": {
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.1",
    "express": "*",
    "node-fetch": "^2.6.1",
    "xterm": "^4.3.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.8.0",
    "ember-cli": "~3.23.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.23.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.15.0",
    "eslint-plugin-ember": "^10.0.2",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.6.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "broccoli-serve-files"
  }
}
